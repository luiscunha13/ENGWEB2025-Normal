extends layout

block content
  .w3-card-4
    header.w3-container.w3-blue 
      h1= title
    .w3-container
    table.w3-table-all
        tr
            th id
            th Ano
            th País Organizador
            th País Vencedor
        each edicao in edicoes
            tr
                td
                    a(href=`/${edicao._id}`)= edicao._id
                td= edicao.anoEdição
                td
                    a(href=`/paises/${edicao.organizacao}`)= edicao.organizacao
                td
                    a(href=`/paises/${edicao.vencedor}`)= edicao.vencedor
    
    footer.w3.container.w3-blue 
      adress Generated by EngWeb2025 class